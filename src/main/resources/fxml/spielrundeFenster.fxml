                            </rowConstraints>
                            <children>

                                <Label fx:id="textAusgabe" alignment="CENTER" maxHeight="75.0" maxWidth="300.0"
                                       minHeight="75.0" minWidth="300.0" prefHeight="75.0" prefWidth="300.0"
                                       styleClass="label" text="TEXT">
                                    <padding>
                                        <Insets left="50.0" right="50.0"/>
                                    </padding>
                                </Label>

                                <Button fx:id="weiterKnopf" maxHeight="75.0" maxWidth="300.0" minHeight="75.0"
                                        minWidth="300.0" mnemonicParsing="false" onAction="#szeneWechsel"
                                        onMouseEntered="#knopfZielen" prefHeight="75.0" prefWidth="300.0"
                                        styleClass="buttons" text="WEITER" GridPane.rowIndex="1">
